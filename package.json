{
  "name": "epsagon-cdk",
  "author": "Epsagon Team <support@epsagon.com>",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/epsagon/epsagon-cdk.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "tslint 'lib/**/*.ts{,x}'",
    "build": "jsii",
    "watch": "tsc -w",
    "package": "jsii-pacmak -v",
    "test": "jest",
    "deploy": "./scripts/deploy.sh"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.epsagon.cdk",
        "maven": {
          "groupId": "com.epsagon.cdk",
          "artifactId": "epsagon"
        }
      },
      "dotnet": {
        "namespace": "Epsagon.Cdk",
        "packageId": "Epsagon.Cdk"
      },
      "sphinx": {}
    }
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.109.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "jsii": "^1.32.0",
    "jsii-pacmak": "^1.32.0",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7"
  },
  "peerDependencies": {
    "@aws-cdk/core": "1.109.0",
    "@aws-cdk/aws-lambda": "^1.115.0"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda": "^1.115.0",
    "@aws-cdk/core": "1.109.0"
  }
}
